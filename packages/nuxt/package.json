{
  "name": "nuxt",
  "version": "3.12.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/nuxt.git",
    "directory": "packages/nuxt"
  },
  "homepage": "https://nuxt.com",
  "description": "Nuxt is a free and open-source framework with an intuitive and extendable way to create type-safe, performant and production-grade full-stack web applications and websites with Vue.js.",
  "license": "MIT",
  "type": "module",
  "types": "./types.d.ts",
  "bin": {
    "nuxi": "bin/nuxt.mjs",
    "nuxt": "bin/nuxt.mjs"
  },
  "exports": {
    ".": {
      "types": "./types.d.mts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./config.d.ts",
      "import": "./config.js",
      "require": "./config.cjs"
    },
    "./schema": {
      "types": "./schema.d.ts",
      "import": "./schema.js"
    },
    "./kit": {
      "types": "./kit.d.ts",
      "import": "./kit.js"
    },
    "./app": {
      "types": "./dist/app/index.d.ts",
      "import": "./dist/app/index.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#app": {
      "types": "./dist/app/index.d.ts",
      "import": "./dist/app/index.js"
    }
  },
  "files": [
    "app.d.ts",
    "bin",
    "types.d.ts",
    "types.d.mts",
    "dist",
    "config.*",
    "kit.*",
    "schema.*"
  ],
  "scripts": {
    "prepack": "unbuild"
  },
  "dependencies": {
    "@nuxt/devalue": "catalog:",
    "@nuxt/devtools": "catalog:",
    "@nuxt/kit": "workspace:*",
    "@nuxt/schema": "workspace:*",
    "@nuxt/telemetry": "^2.5.4",
    "@nuxt/vite-builder": "workspace:*",
    "@unhead/dom": "catalog:",
    "@unhead/ssr": "catalog:",
    "@unhead/vue": "catalog:",
    "@vue/shared": "catalog:",
    "acorn": "catalog:",
    "c12": "^2.0.0-beta.1",
    "chokidar": "catalog:",
    "compatx": "catalog:",
    "consola": "^3.2.3",
    "cookie-es": "catalog:",
    "defu": "catalog:",
    "destr": "catalog:",
    "devalue": "^5.0.0",
    "esbuild": "catalog:",
    "escape-string-regexp": "catalog:",
    "estree-walker": "catalog:",
    "globby": "^14.0.2",
    "h3": "catalog:",
    "hookable": "catalog:",
    "ignore": "^5.3.1",
    "jiti": "^2.0.0-beta.3",
    "klona": "catalog:",
    "knitwork": "^1.1.0",
    "magic-string": "catalog:",
    "mlly": "catalog:",
    "nitro": "catalog:",
    "nuxi": "^3.12.0",
    "nypm": "catalog:",
    "ofetch": "^1.3.4",
    "ohash": "catalog:",
    "pathe": "^1.1.2",
    "perfect-debounce": "catalog:",
    "pkg-types": "catalog:",
    "radix3": "catalog:",
    "scule": "^1.3.0",
    "semver": "^7.6.2",
    "std-env": "^3.7.0",
    "strip-literal": "catalog:",
    "ufo": "^1.5.3",
    "ultrahtml": "catalog:",
    "uncrypto": "catalog:",
    "unctx": "^2.3.1",
    "unenv": "^1.9.0",
    "unimport": "catalog:",
    "unplugin": "catalog:",
    "unplugin-vue-router": "catalog:",
    "unstorage": "catalog:",
    "untyped": "catalog:",
    "vue": "^3.4.31",
    "vue-bundle-renderer": "^2.1.0",
    "vue-devtools-stub": "catalog:",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@nuxt/scripts": "catalog:",
    "@nuxt/ui-templates": "catalog:",
    "@parcel/watcher": "catalog:",
    "@types/estree": "catalog:",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/compiler-sfc": "catalog:",
    "unbuild": "catalog:",
    "vite": "5.3.3",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.1.0",
    "@types/node": "^14.18.0 || >=16.10.0"
  },
  "peerDependenciesMeta": {
    "@parcel/watcher": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  }
}

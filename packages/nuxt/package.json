    "estree-walker": "catalog:",
    "globby": "catalog:",
    "h3": "catalog:",
    "hookable": "catalog:",
    "ignore": "catalog:",
    "impound": "catalog:",
    "jiti": "catalog:",
    "klona": "catalog:",
    "knitwork": "catalog:",
    "magic-string": "catalog:",
    "mlly": "catalog:",
    "nanotar": "catalog:",
    "nitro": "catalog:",
    "nypm": "catalog:",
    "ofetch": "catalog:",
    "ohash": "catalog:",
    "pathe": "catalog:",
    "perfect-debounce": "catalog:",
    "pkg-types": "catalog:",
    "radix3": "catalog:",
    "scule": "catalog:",
    "semver": "catalog:",
    "std-env": "catalog:",
    "strip-literal": "catalog:",
    "tinyglobby": "catalog:",
    "ufo": "catalog:",
    "ultrahtml": "catalog:",
    "uncrypto": "catalog:",
    "unctx": "catalog:",
    "unenv": "catalog:",
    "unhead": "catalog:",
    "unimport": "catalog:",
    "unplugin": "catalog:",
    "unplugin-vue-router": "catalog:",
    "unstorage": "catalog:",
    "untyped": "catalog:",
    "vue": "catalog:",
    "vue-bundle-renderer": "catalog:",
    "vue-devtools-stub": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@nuxt/scripts": "catalog:",
    "@parcel/watcher": "catalog:",
    "@types/estree": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/compiler-sfc": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.1.0",
    "@types/node": ">=18.12.0"
  },
  "peerDependenciesMeta": {
    "@parcel/watcher": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    }
  },
  "engines": {
    "node": "^18.20.5 || ^20.9.0 || >=22.0.0"
  }
}

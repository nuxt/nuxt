  "devDependencies": {
    "@nuxt/schema": "workspace:*",
    "@types/pify": "catalog:",
    "@types/webpack-bundle-analyzer": "catalog:",
    "@types/webpack-hot-middleware": "catalog:",
    "rollup": "catalog:",
    "unbuild": "catalog:",
    "vue": "catalog:"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "engines": {
    "node": "^18.12.0 || ^20.9.0 || >=22.0.0"
  }

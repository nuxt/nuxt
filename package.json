{
  "name": "nuxt",
  "version": "0.10.6",
  "description": "A minimalistic framework for server-rendered Vue.js applications (inspired by Next.js)",
  "contributors": [
    {
      "name": "Sebastien Chopin"
    },
    {
      "name": "Alexandre Chopin"
    }

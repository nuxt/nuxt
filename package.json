{
  "name": "nuxt-framework",
  "private": true,
  "repository": "nuxt/nuxt",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm --filter './packages/**' prepack",
    "build:stub": "pnpm --filter './packages/**' prepack --stub",
    "cleanup": "rimraf 'packages/**/node_modules' 'examples/**/node_modules' 'docs/node_modules' 'playground/node_modules' 'node_modules'",
    "dev": "pnpm play",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "lint:fix": "eslint --ext .vue,.ts,.js,.mjs . --fix",
    "lint:docs": "markdownlint ./docs && case-police 'docs/**/*.md'",
    "lint:docs:fix": "markdownlint ./docs --fix && case-police 'docs/**/*.md' --fix",
    "play": "nuxi dev playground",
    "play:build": "nuxi build playground",
    "play:preview": "nuxi preview playground",
    "test": "pnpm test:fixtures && pnpm test:fixtures:payload && pnpm test:fixtures:dev && pnpm test:fixtures:webpack && pnpm test:unit && pnpm typecheck",
    "test:fixtures": "nuxi prepare test/fixtures/basic && nuxi prepare test/fixtures/runtime-compiler && vitest run --dir test",
    "test:fixtures:payload": "TEST_PAYLOAD=js pnpm test:fixtures",
    "test:fixtures:dev": "TEST_ENV=dev pnpm test:fixtures",
    "test:fixtures:webpack": "TEST_BUILDER=webpack pnpm test:fixtures",
    "test:types": "nuxi prepare test/fixtures/basic && cd test/fixtures/basic && npx vue-tsc --noEmit",
    "test:unit": "vitest run --dir packages",
    "typecheck": "tsc --noEmit"
  },
  "resolutions": {
    "@nuxt/kit": "workspace:*",
    "@nuxt/schema": "workspace:*",
    "@nuxt/test-utils": "workspace:*",
    "@nuxt/vite-builder": "workspace:*",
    "@nuxt/webpack-builder": "workspace:*",
    "nuxi": "workspace:*",
    "nuxt": "workspace:*",
    "vite": "^4.3.8",
    "vue": "3.3.4",
    "magic-string": "^0.30.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@nuxt/test-utils": "workspace:*",
    "@nuxt/webpack-builder": "workspace:*",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/crawler": "^1.2.2",
    "@types/node": "^18.16.12",
    "@types/semver": "^7.5.0",
    "case-police": "^0.6.1",
    "chalk": "^5.2.0",
    "changelogen": "^0.5.3",
    "consola": "^3.1.0",
    "crawler": "^1.4.0",
    "devalue": "^4.3.1",
    "eslint": "^8.40.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^41.1.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "h3": "^1.6.6",
    "jiti": "^1.18.2",
    "markdownlint-cli": "^0.33.0",
    "nuxi": "workspace:*",
    "nuxt": "workspace:*",
    "ofetch": "^1.0.1",
    "pathe": "^1.1.0",
    "playwright": "^1.33.0",
    "rimraf": "^5.0.1",
    "semver": "^7.5.1",
    "std-env": "^3.3.3",
    "typescript": "^5.0.4",
    "ufo": "^1.1.2",
    "vite": "^4.3.8",
    "vitest": "^0.31.1",
    "vue": "3.3.4",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  }
}

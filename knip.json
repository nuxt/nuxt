{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [
        "scripts/*",
        "test/*",
        "test/fixtures/*"
      ]
    },
    "packages/*": {
      "entry": [
        "src/index.ts",
        "src/runtime/**/*.ts"
      ]
    },
    "packages/nuxt": {
      "entry": [
        "src/app/**/*.ts",
        "src/app/*.ts",
        "src/*/runtime/**/*.ts",
        "src/core/templates.ts",
        "src/index.ts"
      ]
    }
  }
}
